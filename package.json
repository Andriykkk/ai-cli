{
  "name": "ai-cli",
  "version": "0.1.0",
  "description": "AI-powered CLI tool for code development with React UI",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "ai-cli": "dist/index.js"
  },
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "tsx watch src/index.ts",
    "client": "tsx src/client/index.ts",
    "dev:client": "tsx watch src/client/index.ts",
    "server": "cd server && python main.py",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "ink": "^5.2.1",
    "ink-spinner": "^5.0.0",
    "react": "^18.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
